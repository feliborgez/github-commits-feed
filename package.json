{
  "name": "@stalkr-io/github",
  "version": "0.1.0",
  "description": "Stalkr Github webhook.",
  "license": "MIT",
  "repository": {},
  "author": "matt <@ythecombinator>",
  "keywords": [
    ""
  ],
  "files": [
    "dist/"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "start": "npm run code:run",
    "code:run": "node ./dist/",
    "code:build": "node tasks/bundle",
    "code:watch": "node tasks/bundle --watch",
    "code:test:spec": "yarn build && mocha --compilers ts:ts-node/register --recursive 'test/**/*-spec.ts'",
    "code:test:spec:coverage": "nyc --reporter=text --reporter=html --reporter=lcov mocha --compilers ts:ts-node/register",
    "code:test:lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "docs:code": "typedoc --out dist/docs",
    "docs:TOC": "markdown-toc README.md -i",
    "docs:TOC:watch": "onchange 'README.md' -- npm run generate:main",
    "all:watch": "run-p code:watch docs:TOC:watch",
    "clean": "rimraf lib"
  },
  "dependencies": {
    "@types/es6-promise": "0.0.32",
    "@types/express": "^4.0.35",
    "@types/ramda": "0.0.7",
    "@types/socket.io": "^1.4.29",
    "axios": "^0.16.1",
    "babel-preset-latest": "^6.16.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.2",
    "micro": "^7.3.3",
    "monet": "^0.8.10",
    "pusher": "^1.5.1",
    "ramda": "^0.23.0",
    "socket.io": "^1.7.3",
    "source-map-support": "^0.4.9"
  },
  "devDependencies": {
    "@types/bluebird": "^3.5.3",
    "@types/chai": "^3.0.0",
    "@types/mocha": "^2.0.0",
    "@types/node": "^7.0.0",
    "awesome-typescript-loader": "^3.1.3",
    "bluebird": "^3.5.0",
    "chai": "^3.0.0",
    "chalk": "^1.1.3",
    "coveralls": "^2.0.0",
    "markdown-toc": "^1.1.0",
    "mocha": "^3.0.0",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "nyc": "^10.0.0",
    "onchange": "^3.2.1",
    "ora": "^1.2.0",
    "rimraf": "^2.0.0",
    "source-map-loader": "^0.2.1",
    "ts-node": "^3.0.0",
    "tslint": "^5.0.0",
    "typedoc": "^0.6.0",
    "typescript": "^2.3.2",
    "webpack": "^2.5.1",
    "yargs": "^7.1.0"
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "nyc": {
    "include": [
      "src/*.ts"
    ],
    "exclude": [
      "dist"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [],
    "all": true
  }
}
